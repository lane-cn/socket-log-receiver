<configuration>
	<contextName>socket-log-receiver</contextName>
	<include resource="org/springframework/boot/logging/logback/base.xml" />
	<jmxConfigurator />
	<receiver class="ch.qos.logback.classic.net.server.ServerSocketReceiver">
		<port>4560</port>
	</receiver>
</configuration>
